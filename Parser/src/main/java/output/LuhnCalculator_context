import java.io.Serializable;
import java.util.List;
import java.io.BufferedInputStream;
class InnerClass {
    private int x;
    private int y;

    private InnerClass(int x, int y) {
        this.x = x;
        this.y = y;
    }

    private int[] getXY(){
        int[] vars = {x, y};
        return vars;
    }
   }
